<%= render 'documents/search_documents', document: @document %>
<div class="document-contents">
  <% @documents.each do |document| %>
    <div class="documents">
      <%= link_to document.title, document_path(document.id), method: :get, class: "document-name" %>
      <p><%= document.jurisdiction_department %></p>
      <p><%= document.disposal_date %></p>
    </div>
  <% end %>
</div>